#include util.p8

local a=shuffle({2,3,3,4})

::_::
cls()
print_arr(a,5,5,7)
flip()
goto _

function print_arr(a,x,y,c)
	local str="["
	local n=#a
	for i=1,n do
		str=str..tostr(a[i])
		if (i<n) str=str..","
	end
	str=str.."]"
	print(str,x,y,c)
end
