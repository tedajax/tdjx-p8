pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
function _init()
	ft=30
end

function _update()
	ft-=1
	if ft<=0 then
		ft=2
		for x=0,15 do
			for y=0,15 do
				local m=mget(x,y)
				if m>=8 and m<=11 then
					mset(x,y,m+1)
				end
			end
		end
	end	
end

function _draw()
	cls()
	map(0,0,0,0,16,16)
	spr(32,32,128-24,2,2)
end
