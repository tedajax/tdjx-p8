function _init()
	cls()
	s=flr(rnd(32767))
end

function _update60()
end

function _draw()
	srand(s)
	local u=t()/4
	local m=300
	for i=0,m-1 do
		local a=u+rnd()
		x,y,z=rnd(72),rnd(),rnd(16)
		if flr(t()%m)==i then
			local r=x+sin(u/y)*z
			pset(64+cos(a)*r,64+sin(a)*r,(i%7)+8)
		end
	end
end
