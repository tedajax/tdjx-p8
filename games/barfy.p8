-- state stack

state_stack={}

_valid_state_keys={
	name=true,
	update=true,
	draw=true,
	key=true,
}
function push_s(state)
	-- assert all state
	-- keys are in approved list
	-- for sanity checking during
	-- development
	for k,v in pairs(state) do
		assert(_valid_state_keys[k])
	end
		
	return add(state_stack,state)
end

function pop_s()
	local n=#state_stack	
	assert(n>0)
	local ret=state_stack[n]
	state_stack[n]=nil
	return ret
end

function pop_s_to(s_name)
	local n=#state_stack
	local i=n
	while i>0 do
		local s=state_stack[i]
		if s.name==s_name then
			break
		end
		i-=1
	end
	assert(i>0)
	for j=i,n do
		state_stack[j]=nil
	end
end

function get_top_s()
	local n=#state_stack
	return state_stack[n]
end
