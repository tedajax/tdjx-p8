function init_play()
	actors={}
	cam={x=0,y=0}
	world_init(0,0,32,16)
	push_s({
		name="update",
		update=play_update,
		draw=play_draw
	})
end

function play_update()
	local ix,iy=0,0
	if (btn(0)) ix-=1
	if (btn(1)) ix+=1
	if (btn(2)) iy-=1
	if (btn(3)) iy+=1

	player.input={
		x=ix,y=iy,j=btnp(4)
	}
	
	if btnp(5) then
		barf()
	end

	for actor in all(actors) do
		actor:move()
	end
	
	cam.x=(player.x-8)*8
end

function play_draw()
	cls()
	
	camera(cam.x,cam.y)
	map(world.x,world.y,0,0,world.w,world.h)
	
	for actor in all(actors) do
		actor:draw()
	end

	camera()
end



function barf()
	add(actors,splash:new({
		kind=32,
		owner=player,
		ox=0,oy=-1.5,
		face=player.face,
		dx=player.dx,
		dy=mid(player.dy,-0.2,0.2)
	}))
end
