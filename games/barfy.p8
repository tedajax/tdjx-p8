function sequence(func,param)
	add(sequences,{
		cr=cocreate(func),
		param=param or {}
	})
end

function sequences_update()
	for _,seq in ipairs(sequences) do
		if costatus(seq.cr)~="dead"
		then
			_sparam=seq.param
			coresume(seq.cr)
		else
			del(sequences,seq)
		end
	end
end
