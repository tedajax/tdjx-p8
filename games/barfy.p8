	if self.t0<4 then
		self.t0+=0.66
	else
		self.t0+=0.33
	end
	
	if self.t0<3 then
		self.x+=self.face*0.2	
	else
		self.x+=self.face*0.05
	end
	
	if self.t0>=1 and self.t0<=4
	then
		local x0,y0,x1,y1=
			self:world_bounds()
	
		for xx=x0,x1,0.125 do
			for yy=y0,y1,0.125 do
