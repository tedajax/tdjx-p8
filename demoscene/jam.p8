pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
e=2.713
::_::
cls(1)
srand()
u=t()/8
for i=0,100 do
s=i*e*0.2
j=-i/100+rnd()
x=64+cos(i*e-u+j)*s
y=64+sin(i*e-u+j)*s
for n=0,14 do
a=n/4*e-u
r=n/(e*e)*8
z=cos(a+u*e)*s/e
w=sin(a+u*e)*s/e
circfill(x+cos(a)*r+z,y+sin(a)*r+w,rnd(e*1.4),7)
end
end
flip()goto _
