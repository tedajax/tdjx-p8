_dbg={
	watch={},
	log={}
}

function watch(msg,col)
	local m={
		msg=tostr(msg),
		col=col or 11
	}
	add(_dbg.watch,m)
end

function log(msg,col)
	local m={
		msg=tostr(msg),
		col=col or 7
	}
	add(_dbg.log,m)
	local n=#_dbg.log
	if n>20 then
		for i=1,20 do
			_dbg.log[i]=_dbg.log[i+1]
		end
		_dbg.log[21]=nil
	end
end

function dbg_draw()
	local n=#_dbg.watch
	for i=1,n do
		local w=_dbg.watch[i]
		print(w.msg,0,(i-1)*6,w.col)
	end
	
	local n=#_dbg.log
	for i=1,n do
		local l=_dbg.log[i]
		local len=#l.msg*4
		print(l.msg,127-len,(i-1)*6,l.col)
	end
	
	_dbg.watch={}
end
