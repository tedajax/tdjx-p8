function solid(x,y)
	return fget(mget(x,y),0)
end

function world_init(x,y,w,h)
	world={x=x,y=y,w=w,h=h}

	for xx=x,x+w-1 do
		for yy=y,y+h-1 do
			if mget(xx,yy)==8 then
				player=make_actor(8,
					{x=xx+0.5,y=yy+1})
				mset(xx,yy,0)
			end
		end
	end
end
