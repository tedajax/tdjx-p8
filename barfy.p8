function _init()
	music(0)
	actors={}
	cam={x=0,y=0}
	world_init(0,0,32,16)
end

function barf()
	add(actors,splash:new({
		kind=32,
		x=player.x,
		y=player.y-0.5,
		face=player.face,
		dx=player.dx,
		dy=mid(player.dy,-0.2,0.2)
	}))
end

function _update()
	local ix,iy=0,0
	if (btn(0)) ix-=1
	if (btn(1)) ix+=1
	if (btn(2)) iy-=1
	if (btn(3)) iy+=1

	player.input={
		x=ix,y=iy,j=btnp(4)
	}

	for actor in all(actors) do
		actor:move()
	end
	
	cam.x=(player.x-8)*8
end

function _draw()
	cls()
	
	camera(cam.x,cam.y)
	map(world.x,world.y,0,0,world.w,world.h)
	
	for actor in all(actors) do
		actor:draw()
	end

	camera()
	
	dbg_draw()
end

function class(new,base)
	new=new or {}
	
	function new:new(p)
		self.__index=self
		return
			setmetatable(p or {},self)
	end
	
	if base then
		setmetatable(new,{
			__index=base
		})
	end
	
	return new
end

